<%- include('./partials/layout') %> 

<h2 class="page-header">Product Categories</h2>

<main>

<ul class="category-list">
  <li>
    <a href="category/create">+ New Category</a>
  </li>
  <% for(const category of categories) { %> 
    <li>
      <a href=<%= category.url%> >
        <%= category.name %> 
      </a>
    </li>
    <% } %> 
</ul>
</main>

<%- include('./partials/footer') %> 

</body>
</html>